# COVID-19 Data Analytics Platform - Environment Configuration
# Copy this file to .env and fill in your actual values

# Snowflake Configuration
SNOWFLAKE_ACCOUNT=your_account.region.cloud
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=COVID19_EPIDEMIOLOGICAL_DATA
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_ROLE=your_role

# MongoDB Configuration
# For cloud MongoDB (recommended):
# MONGODB_URI=mongodb+srv://username:password@your-cluster.mongodb.net/
# For local MongoDB:
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB=COVID_COMMENTS

# API Configuration
API_HOST=0.0.0.0
API_PORT=8003
API_BASE_URL=http://localhost:8003